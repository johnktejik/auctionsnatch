<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ 
..\..\FrameXML\UI.xsd"> 
<Frame name="ASframe">
	<Scripts>
		<OnLoad>
			AS_OnLoad(self);
		</OnLoad>
		<OnEvent>
			AS_OnEvent(self,event);
		</OnEvent>
		<OnUpdate>
			AS_OnUpdate(self,elapsed);
		</OnUpdate>
	</Scripts>
</Frame>
</Ui> 